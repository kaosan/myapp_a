<div class = "container">
<div class ="t_menu">
<h1>ログイン</h1>
<br>
<p><%= notice %></p>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
<div class="user_table">
<table class = "user_form">
<tr>
<div class ="login">
<th><%= f.label :メールアドレス %></th>
<th><%= f.email_field :email %></th>
</div>
</tr>
<br>
<tr>
<div class="login">
<th><%= f.label :パスワード %></th>
<th><%= f.password_field :password %></th>
</div>
</tr>
</table>
</div>
<br>
<%= f.submit "ログイン", class: 'botton' %>
<% end %>
</div>
</div>
